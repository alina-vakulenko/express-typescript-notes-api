{
  "name": "express-notes-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "tsc --project tsconfig.build.json && tsc-alias",
    "start:prod": "npm run build && node build/index.js",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only src/index.ts",
    "migration-template": "npx sequelize-cli migration:generate --name migration-skeleton",
    "migrate:up": "npm run build && npx sequelize-cli db:migrate",
    "migrate:undo": "npx sequelize-cli db:migrate:undo",
    "seed": "npx sequelize-cli db:seed:all",
    "test": "jest --coverage --detectOpenHandles"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "cors": "^2.8.5",
    "dayjs": "^1.11.8",
    "dotenv": "^16.1.4",
    "envalid": "^7.3.1",
    "express": "^4.18.2",
    "pg": "^8.11.2",
    "pg-hstore": "^2.3.4",
    "pino": "^8.14.1",
    "pino-pretty": "^10.0.0",
    "sequelize": "^6.32.1",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.5",
    "@types/node": "^20.4.8",
    "@types/pino": "^7.0.5",
    "@types/sequelize": "^4.28.15",
    "@types/supertest": "^2.0.12",
    "@types/uuid": "^9.0.2",
    "jest": "^29.5.0",
    "sequelize-cli": "^6.6.1",
    "supertest": "^6.3.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.7",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.3",
    "zod": "^3.21.4"
  }
}
